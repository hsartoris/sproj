#!/bin/bash
for i in `seq 1 100`; do
    runModel run 2simplex25k simple $i
    cp model/checkpoints/3neurdata/simple/$i/losses model/checkpoints/3neurdata/losses/simple/
    runModel run 2simplex25k conv $i
    cp model/checkpoints/3neurdata/conv/$i/losses model/checkpoints/3neurdata/losses/conv/
done
